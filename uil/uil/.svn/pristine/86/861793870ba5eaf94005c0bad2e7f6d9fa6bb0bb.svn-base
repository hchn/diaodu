package com.jiaxun.uil.model;

import com.jiaxun.sdk.scl.model.ConfMemModel;

/**
 * 说明：会议成员元素
 *
 * @author  hubin
 *
 * @Date 2015-4-22
 */
public class ConfMemberItem extends BaseListItem
{
    private static final long serialVersionUID = 1L;
    
    private String confId;
    
    private ConfMemModel confMemModel;
    // 重拨次数
    private int recallTimes;
    
    public ConfMemberItem()
    {
    }
    
    public int getRecallTimes()
    {
        return recallTimes;
    }

    public void setRecallTimes(int recallTimes)
    {
        this.recallTimes = recallTimes;
    }

    public ConfMemModel getConfMemModel()
    {
        return confMemModel;
    }

    public void setConfMemModel(ConfMemModel confMemModel)
    {
        this.confMemModel = confMemModel;
    }

    public String getConfId()
    {
        return confId;
    }

    public void setConfId(String confId)
    {
        this.confId = confId;
    }
}
