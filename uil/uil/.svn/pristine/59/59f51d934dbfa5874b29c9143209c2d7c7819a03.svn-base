<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/title_footer_layout_height"
    android:orientation="horizontal" >

 

    <View
        android:layout_width="0.8dp"
        android:layout_height="match_parent"
        android:background="@color/call_record_item_color" />
   <ImageView
        android:id="@+id/conf_imageview"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:contentDescription="@null"
        android:layout_marginLeft="5dp"
        android:layout_gravity="center_vertical"
        android:src="@drawable/setting_record_conf_icon"
        android:visibility="invisible" />
    <TextView
        android:id="@+id/caller_num_textview"
        style="@style/fontStyleWhiteMedium"
        android:layout_width="150dp"
        android:layout_height="match_parent"
        android:ellipsize="end"
        android:gravity="center_vertical"
        android:maxLines="1" />

    <View
        android:layout_width="0.8dp"
        android:layout_height="match_parent"
        android:background="@color/call_record_item_color" />

    <TextView
        android:id="@+id/caller_name_textview"
        style="@style/fontStyleWhiteMedium"
        android:layout_width="150dp"
        android:layout_marginLeft="5dp"
        android:layout_height="match_parent"
        android:ellipsize="end"
        android:gravity="center_vertical"
        android:maxLines="1" />

    <View
        android:layout_width="0.8dp"
        android:layout_height="match_parent"
        android:background="@color/call_record_item_color" />

    <TextView
        android:id="@+id/peer_num_textview"
        style="@style/fontStyleWhiteMedium"
        android:layout_width="150dp"
        android:layout_marginLeft="5dp"
        android:layout_height="match_parent"
        android:ellipsize="end"
        android:gravity="center_vertical"
        android:maxLines="1" />

    <View
        android:layout_width="0.8dp"
        android:layout_height="match_parent"
        android:background="@color/call_record_item_color" />

    <TextView
        android:id="@+id/peer_name_textview"
        style="@style/fontStyleWhiteMedium"
        android:layout_width="150dp"
        android:layout_marginLeft="5dp"
        android:layout_height="match_parent"
        android:ellipsize="end"
        android:gravity="center_vertical"
        android:maxLines="1" />

    <View
        android:layout_width="0.8dp"
        android:layout_height="match_parent"
        android:background="@color/call_record_item_color" />

    <TextView
        android:id="@+id/call_type_textview"
        style="@style/fontStyleWhiteMedium"
        android:layout_width="100dp"
        android:layout_marginLeft="5dp"
        android:layout_height="match_parent"
        android:ellipsize="end"
        android:gravity="center_vertical"
        android:maxLines="1" />

    <View
        android:layout_width="0.8dp"
        android:layout_height="match_parent"
        android:background="@color/call_record_item_color" />

    <TextView
        android:id="@+id/start_time_textview"
        style="@style/fontStyleWhiteMedium"
        android:layout_marginLeft="5dp"
        android:layout_width="200dp"
        android:layout_height="match_parent"
        android:ellipsize="end"
        android:gravity="center_vertical"
        android:maxLines="1" />

    <View
        android:layout_width="0.8dp"
        android:layout_height="match_parent"
        android:background="@color/call_record_item_color" />

    <TextView
        android:id="@+id/connect_time_textview"
        style="@style/fontStyleWhiteMedium"
        android:layout_width="200dp"
        android:layout_marginLeft="5dp"
        android:layout_height="match_parent"
        android:ellipsize="end"
        android:gravity="center_vertical"
        android:maxLines="1" />

    <View
        android:layout_width="0.8dp"
        android:layout_height="match_parent"
        android:background="@color/call_record_item_color" />

    <TextView
        android:id="@+id/release_time_textview"
        style="@style/fontStyleWhiteMedium"
        android:layout_width="200dp"
        android:layout_marginLeft="5dp"
        android:layout_height="match_parent"
        android:ellipsize="end"
        android:gravity="center_vertical"
        android:maxLines="1" />

    <View
        android:layout_width="0.8dp"
        android:layout_height="match_parent"
        android:background="@color/call_record_item_color" />

    <TextView
        android:id="@+id/call_priority_textview"
        style="@style/fontStyleWhiteMedium"
